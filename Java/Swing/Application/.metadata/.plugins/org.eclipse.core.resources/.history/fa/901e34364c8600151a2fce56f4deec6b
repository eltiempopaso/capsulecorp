public class FrameSocket implements IFrame {
	
	/// Constructor
	private ServerSocket listener_; 
	protected FrameSocket (int port) throws IOException
	{
		listener_ = new ServerSocket(port);
		System.out.println("SocketManager created");
	}
	public static FrameSocket getInstance () {
		if (instance_ == null)
		{
			try {
				instance_ = new FrameSocket(9999);
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
		return instance_;
	}
	
	@Override
	public void setOnEvent(IFrameSuscriptor subscriptor) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public boolean sendCommand(int command, int parameter) {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public boolean sendEvent(int event, int prameter) {
		// TODO Auto-generated method stub
		return false;
	}
}
